{
  "manifest_version": 2,
  "name": "image randomizer",
  "version": "1.0",
  "description": "replace all images on the page with a random image",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.1.1.min.js","./main.js"]
    }
  ]
}
